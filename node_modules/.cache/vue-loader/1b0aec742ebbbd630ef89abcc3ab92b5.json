},\r\n    getProductMostSell() {\r\n      axios\r\n        .post(`${this.$URL}/Admin/Product.php`, {\r\n          action: \"getProductShowMostSell\",\r\n          id_user: this.$session.get(\"id_member\"),\r\n        })\r\n        .then((res) => {\r\n          this.ProductMostSell = res.data;\r\n        });\r\n    },\r\n    getSlide() {\r\n      axios\r\n        .post(`${this.$URL}/Admin/Slide.php`, {\r\n          action: \"getSlide\",\r\n          name_slide: \"Main\",\r\n        })\r\n        .then((res) => {\r\n          this.Slide = res.data;\r\n        });\r\n    },\r\n    getProductAll() {\r\n      this.getProductNew();\r\n      this.getProductView();\r\n      this.getProductMostSell();\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.getType();\r\n    this.getBrand();\r\n    this.getProductAll();\r\n    this.getSlide();\r\n    document.title = \"Pepensuda Shop\";\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"]}]}