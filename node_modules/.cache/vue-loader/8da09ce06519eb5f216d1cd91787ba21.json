his.checkEmail = true;\r\n          } else {\r\n            this.checkEmail = false;\r\n          }\r\n        });\r\n    },\r\n\r\n    resetData() {\r\n      this.email = \"\";\r\n      this.password1 = \"\";\r\n      this.password2 = \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ninput {\r\n  border: 0.01rem solid #940b01;\r\n  height: 3rem;\r\n}\r\ninput:focus {\r\n  outline: none;\r\n  box-shadow: none;\r\n  border: 0.01rem solid #003cff;\r\n}\r\n\r\n.btnOk {\r\n  width: 8rem;\r\n  height: 2.7rem;\r\n  background-color: #c5604e;\r\n  color: white;\r\n  border: none;\r\n}\r\n.btnOk:hover {\r\n  color: #ffffff;\r\n  background-color: #d7897d;\r\n}\r\n.btnReset {\r\n  width: 8rem;\r\n  height: 2.7rem;\r\n  background-color: #ffffff;\r\n  color: rgb(0, 0, 0);\r\n  border: 0.01rem solid #c5604e;\r\n}\r\n.btnReset:hover {\r\n  color: #ffffff;\r\n  background-color: #d7897d;\r\n}\r\n.btnForgerpass {\r\n  transition: 0.3s ease;\r\n}\r\n.btnForgerpass:hover {\r\n  color: #c5604e;\r\n}\r\n</style>\r\n"]}]}