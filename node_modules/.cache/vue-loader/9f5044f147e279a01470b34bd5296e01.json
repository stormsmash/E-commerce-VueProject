     \"\r\n            >\r\n              {{ type[1] }} <i class=\"fas fa-chevron-right\"></i>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-8\">\r\n          <div v-for=\"(menu, index) in Type\" :key=\"index\">\r\n            <div v-if=\"activetab === index\">\r\n              <div\r\n                v-for=\"(product, indexP) in Product.filter(\r\n                  (item) => item.productType == menu.id_type\r\n                )\"\r\n                :key=\"indexP\"\r\n                class=\"my-2\"\r\n              >\r\n                <router-link :to=\"`/ProductInfo/${product.id_product}`\">\r\n                  {{ product.productName }}</router-link\r\n                >\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </b-collapse>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"MenuItem\",\r\n  data() {\r\n    return {\r\n      activetab: 2,\r\n      Type: [],\r\n      Product: [],\r\n    };\r\n  },\r\n  methods: {\r\n    getType() {\r\n      axios\r\n        .post(`${this.$URL}/Admin/Type.php`, {\r\n          action: \"getType\",\r\n        })\r\n        .then((res) => {\r\n          this.Type = res.data;\r\n        });\r\n    },\r\n    getProduct() {\r\n      axios\r\n        .post(`${this.$URL}/Admin/Product.php`, {\r\n          action: \"getProductShow\",\r\n        })\r\n        .then((res) => {\r\n          this.Product = res.data;\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    this.getType();\r\n    this.getProduct();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\na {\r\n  text-decoration: none;\r\n  color: #000;\r\n}\r\na:hover {\r\n  color: #35ebc3;\r\n}\r\n.listmenu {\r\n  z-index: 100;\r\n  position: absolute;\r\n  top: 100%;\r\n  left: 0;\r\n  background-color: white;\r\n  width: 100%;\r\n}\r\n.tabs {\r\n  height: 50vh;\r\n  overflow-y: scroll;\r\n}\r\n.tabs::-webkit-scrollbar {\r\n  display: none;\r\n}\r\n.boxBtn {\r\n  background-color: rgb(255, 255, 255);\r\n  width: 50%;\r\n  margin: 0;\r\n  margin-left: 3.75rem;\r\n  border-radius: 0.1rem;\r\n  font-size: 1.1rem;\r\n  cursor: pointer;\r\n  transition: 0.3s all;\r\n  padding: 0.5rem 1.2rem;\r\n}\r\n.boxBtn:hover {\r\n  background-color: #fedbb3;\r\n}\r\n.active {\r\n  background-color: #fedbb3;\r\n}\r\n</style>\r\n"]}]}