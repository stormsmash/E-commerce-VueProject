iv class=\"d-flex justify-content-between align-items-center\">\r\n        <h4 class=\"p-0 m-0\">ยอดรวมสินค้า</h4>\r\n        <div class=\"text-price p-0 m-0\">\r\n          &#3647;\r\n          <animated-number\r\n            :value=\"data\"\r\n            :formatValue=\"formatToPrice\"\r\n            :duration=\"500\"\r\n            class=\"text-price p-0 m-0\"\r\n          />\r\n        </div>\r\n      </div>\r\n      <button @click=\"checkProduct\" class=\"btnCart rounded-pill mx-auto mt-5\">\r\n        ดำเนินการสั่งซื้อสินค้า\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AnimatedNumber from \"animated-number-vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"OrderCart\",\r\n  props: {\r\n    data: Number,\r\n    cartList: Array,\r\n  },\r\n  components: {\r\n    AnimatedNumber,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      id_user: this.$session.get(\"id_member\"),\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    checkProduct() {\r\n      axios\r\n        .post(`${this.$URL}/User/cart.php`, {\r\n          action: \"checkProduct\",\r\n          id_user: this.id_user,\r\n        })\r\n        .then((res) => {\r\n          if (this.deliverPrice >= 20) {\r\n            this.$swal({\r\n              icon: \"error\",\r\n              title: \"น้ำหนักสินค้าเกิน\",\r\n              text: `กรุณาสั่งแยกบิล`,\r\n              showCancelButton: false,\r\n              confirmButtonText: \"ตกลง\",\r\n              confirmButtonColor: \"#EB4247\",\r\n            }).then(() => {\r\n              this.$emit(\"updateCart\");\r\n            });\r\n          } else if (res.data == \"success\") {\r\n            this.$router.push(\"/ConfirmOrder\");\r\n          } else {\r\n            this.$swal({\r\n              icon: \"error\",\r\n              title: \"จำนวนสินค้าไม่เพียงพอ\",\r\n              showCancelButton: false,\r\n              confirmButtonText: \"ตกลง\",\r\n              confirmButtonColor: \"#EB4247\",\r\n            }).then(() => {\r\n              this.$emit(\"updateCart\");\r\n            });\r\n          }\r\n        });\r\n    },\r\n\r\n    formatToPrice(value) {\r\n      var num1 = `${value.toFixed()}`;\r\n      var number = new Intl.NumberFormat().format(num1);\r\n      return number;\r\n    },\r\n  },\r\n  computed: {\r\n    deliverPrice() {\r\n      let sum = 0;\r\n      this.cartList.map((item) => (sum += item.qty * item.productWeigth));\r\n      let weigth = sum / 1000;\r\n      return weigth;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.btnCart {\r\n  background-color: #c5604e;\r\n  border: none;\r\n  font-size: 1.2rem;\r\n  color: white;\r\n  padding: 0.07rem 2rem;\r\n  border-radius: 1.1rem;\r\n  width: 15rem;\r\n  height: 3rem;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n.btnCart:hover {\r\n  background-color: #e8a89c;\r\n  color: white;\r\n}\r\n.boxfixed {\r\n  position: sticky;\r\n  top: 5rem;\r\n}\r\n.text-price {\r\n  font-size: 1.5rem;\r\n  color: #bc2721;\r\n}\r\n</style>\r\n"],"mappings":";AAyBA,OAAAA,cAAA;AACA,OAAAC,KAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACAC,IAAA,EAAAC,MAAA;IACAC,QAAA,EAAAC;EACA;EACAC,UAAA;IACAR;EACA;EAEAI,KAAA;IACA;MACAK,OAAA,OAAAC,QAAA,CAAAC,GAAA;IACA;EACA;EAEAC,OAAA;IACAC,aAAA;MACAZ,KAAA,CACAa,IAAA,SAAAC,IAAA;QACAC,MAAA;QACAP,OAAA,OAAAA;MACA,GACAQ,IAAA,CAAAC,GAAA;QACA,SAAAC,YAAA;UACA,KAAAC,KAAA;YACAC,IAAA;YACAC,KAAA;YACAC,IAAA;YACAC,gBAAA;YACAC,iBAAA;YACAC,kBAAA;UACA,GAAAT,IAAA;YACA,KAAAU,KAAA;UACA;QACA,WAAAT,GAAA,CAAAd,IAAA;UACA,KAAAwB,OAAA,CAAAC,IAAA;QACA;UACA,KAAAT,KAAA;YACAC,IAAA;YACAC,KAAA;YACAE,gBAAA;YACAC,iBAAA;YACAC,kBAAA;UACA,GAAAT,IAAA;YACA,KAAAU,KAAA;UACA;QACA;MACA;IACA;IAEAG,cAAAC,KAAA;MACA,IAAAC,IAAA,MAAAD,KAAA,CAAAE,OAAA;MACA,IAAAC,MAAA,OAAAC,IAAA,CAAAC,YAAA,GAAAC,MAAA,CAAAL,IAAA;MACA,OAAAE,MAAA;IACA;EACA;EACAI,QAAA;IACAnB,aAAA;MACA,IAAAoB,GAAA;MACA,KAAAjC,QAAA,CAAAkC,GAAA,CAAAC,IAAA,IAAAF,GAAA,IAAAE,IAAA,CAAAC,GAAA,GAAAD,IAAA,CAAAE,aAAA;MACA,IAAAC,MAAA,GAAAL